/dts-v1/;

/ {
	compatible = "rockchip,rk3328";

	fragment@0 {
		target-path = "/aliases";

		__overlay__ {
			spi0 = "/spi@ff190000";
		};
	};

	fragment@1 {
		target = <0xffffffff>;

		__overlay__ {
			#address-cells = <0x01>;
			#size-cells = <0x00>;

			spidev@0 {
				compatible = "spidev";
				status = "okay";
				reg = <0x00>;
				spi-max-frequency = <0x4c4b40>;
			};

			spidev@1 {
				compatible = "spidev";
				status = "okay";
				reg = <0x01>;
				spi-max-frequency = <0x4c4b40>;
			};
		};
	};

	__fixups__ {
		spi0 = "/fragment@1:target:0";
	};
};
